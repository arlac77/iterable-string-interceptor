{
  "name": "iterable-string-interceptor",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./src/iterable-string-interceptor.mjs",
  "exports": {
    ".": "./src/iterable-string-interceptor.mjs"
  },
  "description": "Intercept Iterable string - backbone for templates",
  "keywords": [
    "chunk",
    "template"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "ava --timeout 2m",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme ./src/iterable-string-interceptor.mjs --section=API",
    "lint": "documentation lint ./src/iterable-string-interceptor.mjs"
  },
  "devDependencies": {
    "ava": "^3.9.0",
    "c8": "^7.2.0",
    "documentation": "^13.0.1",
    "semantic-release": "^17.1.1"
  },
  "engines": {
    "node": ">=12.16.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/iterable-string-interceptor.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/iterable-string-interceptor/issues"
  },
  "homepage": "https://github.com/arlac77/iterable-string-interceptor#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-esm-only",
      "arlac77/template-github"
    ]
  }
}
