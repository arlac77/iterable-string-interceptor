{
  "name": "iterable-string-interceptor",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "module": "src/iterable-string-interceptor.mjs",
  "description": "Intercept Iterable string - backbone for templates",
  "keywords": [
    "template"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 --temp-directory build/coverage ava && c8 report -r lcov --temp-directory build/coverage",
    "docs": "documentation readme src/iterable-string-interceptor.mjs --section=API",
    "lint": "documentation lint src/iterable-string-interceptor.mjs",
    "posttest": "markdown-doctest",
    "test": "ava"
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "^1.0.1",
    "c8": "^3.2.1",
    "documentation": "^9.1.1",
    "esm": "^3.0.84",
    "markdown-doctest": "^0.9.1",
    "semantic-release": "^15.13.2"
  },
  "engines": {
    "node": ">=10.15.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/iterable-string-interceptor.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/iterable-string-interceptor/issues"
  },
  "homepage": "https://github.com/arlac77/iterable-string-interceptor#readme",
  "ava": {
    "require": [
      "esm"
    ],
    "extensions": [
      "js",
      "mjs"
    ],
    "babel": false,
    "compileEnhancements": false,
    "files": [
      "tests/*-test.js",
      "tests/*-test.mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template-esm-only.git"
    }
  }
}
